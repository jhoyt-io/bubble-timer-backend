{
  "name": "bubble-timer-cdk",
  "version": "0.1.0",
  "bin": {
    "bubble-timer-cdk": "bin/bubble-timer-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:test": "npm run build && npm run test"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@types/jest": "^29.5.12",
    "esbuild": "^0.20.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.699.0",
    "@aws-sdk/client-dynamodb": "^3.569.0",
    "aws-cdk-lib": "^2.140.0",
    "aws-jwt-verify": "^4.0.1",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.ts", "**/*.test.ts"],
    "collectCoverageFrom": [
      "lib/**/*.ts",
      "!lib/**/*.d.ts"
    ]
  }
}
